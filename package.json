{
  "name": "tofil-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "pnpm exec prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "pnpm exec prisma generate --schema=prisma/schema.prisma",
    "createMigrateDev": "pnpm exec zenstack generate && pnpm run prisma:generate && pnpm exec prisma migrate dev --create-only --schema=prisma/schema.prisma --name",
    "prisma:migrate:deploy": "pnpm exec prisma migrate deploy --schema=prisma/schema.prisma",
    "prisma:migrate:deleteSingle": "sh ./delete-migration.sh",
    "prisma:migrate:resolve": "pnpm exec prisma migrate resolve --applied --schema=prisma/schema.prisma --migration",
    "prepare": "husky install",
    "test": "vitest run"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.13",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.49.4",
    "@tabler/icons-react": "^3.31.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.509.0",
    "next": "14.2.28",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "shadcn-ui": "0.3.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8",
    "eslint-config-next": "15.3.2",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "prisma": "5.22.0",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.19.4",
    "typescript": "^5",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3",
    "zenstack": "^2.0.0"
  }
}
