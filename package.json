{
  "type": "module",
  "name": "tofil-app-fresh",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run generate && next dev --turbopack",
    "build": "pnpm run generate && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "generate": "pnpm exec zenstack generate && pnpm exec prisma generate --schema=prisma/schema.prisma",
    "createMigrateDev": "pnpm run generate && pnpm exec prisma migrate dev --create-only --schema=prisma/schema.prisma --name",
    "prisma:migrate:deploy": "pnpm run generate && pnpm exec prisma migrate deploy --schema=prisma/schema.prisma",
    "prisma:migrate:deleteSingle": "sh ./delete-migration.sh",
    "prisma:migrate:resolve": "pnpm run generate && pnpm exec prisma migrate resolve --applied --schema=prisma/schema.prisma --migration",
    "prepare": "husky install",
    "test": "vitest run"
  },
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-toast": "^1.2.13",
    "@shadcn/ui": "^0.0.4",
    "@supabase/supabase-js": "^2.49.4",
    "@tabler/icons-react": "^3.33.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.511.0",
    "next": "15.3.2",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "preset": "link:@shadcn/ui/preset",
    "prisma": "^6.7.0",
    "prisma-json-types-generator": "^3.4.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.0",
    "typescript": "^5",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4",
    "zenstack": "^2.0.0"
  }
}
